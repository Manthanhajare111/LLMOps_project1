{
  "family": "documentportaltd",
  "executionRoleArn": "arn:aws:iam::468284643800:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "containerDefinitions": [
    {
      "name": "document-portal-container",
      "image": "468284643800.dkr.ecr.ap-southeast-2.amazonaws.com/documentportal",
      "essential": true,
      "cpu":1024,
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportaltd",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "mountPoints": [],
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "document-portal-container-8080-tcp",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "API_KEYS",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-2:468284643800:secret:docs_api-k5U2Fo"
        }
      ],
      "systemControls": [],
      "volumesFrom": []
    }
  ]
}
